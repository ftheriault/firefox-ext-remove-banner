{

  "manifest_version": 2,
  "name": "RemoveBanner",
  "version": "1.2",

  "description": "Removes banner preventing reading of some articles",

  "icons": {
    "48": "remove-banner-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.lapresse.ca/*", "*://*.ledevoir.com/*"],
      "js": ["remove-banner.js"]
    }
  ]

}